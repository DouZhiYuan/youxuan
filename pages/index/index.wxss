/* 轮播图 */
.index_swiper swiper{
  width: 750rpx;
  height: 340rpx;
}
.index_swiper image{
  width: 100%;
}

/* 分类导航 */
.index_cate{
  display: flex;
  border-bottom: 10rpx solid #fff;
}
.index_cate navigator{
  flex: 1;
  padding: 30rpx;
}
.index_cate navigator image{
  width: 100%;
  height: 100%;
}

/* 楼层 */
.floor_item{
  padding-top: 20rpx;
  border-bottom: 15rpx solid #fff
}
.floor_item .floor_title image{
  width: 100%;
  padding-left: 20rpx;
}
.floor_item .floor_content{
  overflow: hidden;
}
.floor_item .floor_content navigator{
  float: left;
  width: 33.33vw;
}
.floor_item .floor_content navigator:nth-last-child(-n+4){
  height: calc(33.33vw*386/232/2);
  border-left: 10rpx solid #ffffff;
}
.floor_item .floor_content navigator:nth-last-child(-n+2){
  border-top: 10rpx solid #ffffff;
}
.floor_item .floor_content image{
  width: 100%;
  height: 100%;
}

